  <%= form_with(model: @product) do |f| %>

    <%= form_error_notification(@product) %>

    <%= f.label :title, autofocus: true %><br>
    <%= f.text_field :title %><br>

    <%= f.label :body %><br>
    <%= f.text_area :body %><br>

    <div>
      <%= f.submit 'Save' %>
      <%= link_to 'Cancle', products_path %>
    </div>

  <% end %>